{
  "name": "jlw-color-theme",
  "displayName": "jlw-color-theme",
  "description": "Jerry's Colour Themes",
  "publisher": "Jerry Lee Williams Jr",
  "license": "MIT",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Themes"
  ],
  "keyword": [
    "theme",
    "themes",
    "light"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jwillia3/jlw-color-theme"
   },
  "contributes": {
    "themes": [
      {
        "label": "JLW Light",
        "uiTheme": "vs",
        "path": "./themes/jlw-default-light-color-theme.json"
      },
      {
        "label": "JLW Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/jlw-default-dark-color-theme.json"
      },
      {
        "label": "JLW Dust",
        "uiTheme": "vs",
        "path": "./themes/jlw-dust-light-color-theme.json"
      },
      {
        "label": "JLW Dark Dust",
        "uiTheme": "vs-dark",
        "path": "./themes/jlw-dust-dark-color-theme.json"
      },
      {
        "label": "JLW Contrast",
        "uiTheme": "vs",
        "path": "./themes/jlw-contrast-light-color-theme.json"
      },
      {
        "label": "JLW Dark Contrast",
        "uiTheme": "vs-dark",
        "path": "./themes/jlw-contrast-dark-color-theme.json"
      },
      {
        "label": "JLW Blueprint",
        "uiTheme": "vs",
        "path": "./themes/jlw-blueprint-light-color-theme.json"
      },
      {
        "label": "JLW Dark Blueprint",
        "uiTheme": "vs-dark",
        "path": "./themes/jlw-blueprint-dark-color-theme.json"
      }
    ]
  },
  "scripts": {
    "build": "node src/index.js",
    "clean": "rm -rf themes",
    "package": "vsce package"
  }
}
