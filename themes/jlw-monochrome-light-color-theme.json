{
  "name": "JLW Monochrome Light",
  "colors": {
    "editor.background": "#fff",
    "editor.foreground": "#555",
    "editorLineNumber.foreground": "#bbb",
    "editor.lineHighlightBackground": "#bbb",
    "editorCursor.foreground": "#333",
    "terminalCursor.foreground": "#333",
    "terminal.background": "#fff",
    "terminal.foreground": "#555",
    "terminal.ansiBlack": "#333",
    "terminal.ansiBlue": "#333",
    "terminal.ansiGreen": "#333",
    "terminal.ansiCyan": "#333",
    "terminal.ansiRed": "#333",
    "terminal.ansiMagenta": "#333",
    "terminal.ansiYellow": "#333",
    "terminal.ansiWhite": "#333",
    "terminal.ansiBrightBlack": "#333",
    "terminal.ansiBrightBlue": "#333",
    "terminal.ansiBrightGreen": "#333",
    "terminal.ansiBrightCyan": "#333",
    "terminal.ansiBrightRed": "#333",
    "terminal.ansiBrightMagenta": "#333",
    "terminal.ansiBrightYellow": "#333",
    "terminal.ansiBrightWhite": "#fff",
    "editorIndentGuide.background": "#bbb",
    "widget.shadow": "#333",
    "editorGroupHeader.tabsBorder": "#555",
    "tab.activeBackground": "#aaa",
    "tab.border": "#555",
    "editorBracketHighlight.foreground1": "#333",
    "editorBracketHighlight.foreground2": "#333",
    "editorBracketHighlight.foreground3": "#333",
    "editorBracketHighlight.foreground4": "#333",
    "editorBracketHighlight.foreground5": "#333",
    "editorBracketHighlight.foreground6": "#333"
  },
  "tokenColors": [
    {
      "scope": [
        "variable"
      ],
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#333",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.heading"
      ],
      "settings": {
        "fontStyle": "bold underline"
      }
    },
    {
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.underline"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": [
        "punctuation",
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#555"
      }
    },
    {
      "scope": [
        "variable",
        "punctuation.definition.variable",
        "punctuation.definition.annotation",
        "entity.name.tag",
        "markup.underline.link",
        "markup.list"
      ],
      "settings": {
        "foreground": "#333"
      }
    },
    {
      "scope": [
        "constant",
        "variable.language",
        "string",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "support.constant",
        "entity.other.attribute-name",
        "markup.quote",
        "markup.inline.raw",
        "markup.block.raw",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#333"
      }
    },
    {
      "scope": [
        "entity.name",
        "support.module",
        "support.class"
      ],
      "settings": {
        "foreground": "#333",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#333"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#555",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword",
        "punctuation.definition.keyword",
        "storage.type",
        "storage.modifier",
        "support.type",
        "entity.name.type",
        "support.module.type"
      ],
      "settings": {
        "foreground": "#333"
      }
    },
    {
      "scope": [
        "invalid",
        "string.regexp",
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#333"
      }
    }
  ]
}