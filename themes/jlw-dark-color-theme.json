{
    "name": "Jerry Dark",
    "colors": {
        "editor.background": "#333",
        "editor.foreground": "#aaa",
        "editorCursor.foreground": "#733",
        "terminalCursor.foreground": "#733",
        "terminal.ansiBlack": "#333",
        "terminal.ansiBlue": "#258",
        "terminal.ansiGreen": "#575",
        "terminal.ansiCyan": "#39a",
        "terminal.ansiRed": "#733",
        "terminal.ansiMagenta": "#958",
        "terminal.ansiYellow": "#b73",
        "terminal.ansiWhite": "#aaa",
        "terminal.ansiBrightBlack": "#333",
        "terminal.ansiBrightBlue": "#258",
        "terminal.ansiBrightGreen": "#575",
        "terminal.ansiBrightCyan": "#39a",
        "terminal.ansiBrightRed": "#733",
        "terminal.ansiBrightMagenta": "#958",
        "terminal.ansiBrightYellow": "#b73",
        "terminal.ansiBrightWhite": "#eee",
        "editorIndentGuide.background": "#aaa"
    },
    "tokenColors": [
        {
            "scope": [
                "punctuation"
            ],
            "settings": {
                "foreground": "#aaa"
            }
        },
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#aaa"
            }
        },
        {
            "scope": [
                "entity.name",
                "support.module",
                "support.class"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "storage.type",
                "storage.modifier",
                "support.type",
                "entity.name.type",
                "support.module.type"
            ],
            "settings": {
                "foreground": "#258",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "keyword",
                "punctuation.definition.keyword"
            ],
            "settings": {
                "foreground": "#258"
            }
        },
        {
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#aaa"
            }
        },
        {
            "scope": [
                "variable"
            ],
            "settings": {
                "foreground": "#aaa",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "constant",
                "variable.language",
                "string",
                "punctuation.definition.string.begin",
                "punctuation.definition.string.end",
                "support.constant"
            ],
            "settings": {
                "foreground": "#b73",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "punctuation.section.interpolation.begin",
                "punctuation.section.interpolation.end"
            ],
            "settings": {
                "foreground": "#aaa",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "string.regexp"
            ],
            "settings": {
                "foreground": "#733",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#733",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "invalid"
            ],
            "settings": {
                "foreground": "#733"
            }
        },
        {
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#258"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#258"
            }
        },
        {
            "scope": [
                "text.html",
                "text.xml"
            ],
            "settings": {
                "foreground": "#958"
            }
        },
        {
            "scope": [
                "markup.heading"
            ],
            "settings": {
                "foreground": "#258",
                "fontStyle": "bold underline"
            }
        },
        {
            "scope": [
                "markup.list"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "markup.underline"
            ],
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": [
                "markup.underline.link"
            ],
            "settings": {
                "foreground": "#258",
                "fontStyle": "underline"
            }
        },
        {
            "scope": [
                "markup.quote"
            ],
            "settings": {
                "foreground": "#258"
            }
        },
        {
            "scope": [
                "markup.raw.inline"
            ],
            "settings": {
                "foreground": "#958"
            }
        },
        {
            "scope": [
                "markup.raw.block"
            ],
            "settings": {
                "foreground": "#258"
            }
        }
    ]
}